{
	"name": "builtnorth/wp-schema",
	"description": "Comprehensive schema generation for WordPress with SEO optimization and logo support",
	"version": "0.2.0-beta",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "BuiltNorth",
			"email": "hello@builtnorth.co"
		},
		{
			"name": "Dan Northern",
			"email": "contact@dannorthern.com"
		}
	],
	"require": {
		"php": ">=8.1"
	},
	"autoload": {
		"psr-4": {
			"BuiltNorth\\WPSchema\\": "inc/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"BuiltNorth\\WPSchema\\Tests\\": "tests/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"squizlabs/php_codesniffer": "^3.0",
		"phpcompatibility/php-compatibility": "^9.0"
	},
	"config": {
		"optimize-autoloader": true,
		"sort-packages": true
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
